tree miArbol(1,2,3,4);

configure rbt miArbol {
    addNode(5,6,7,8,9,10);
    findNode(5);
}

create fotoMiArbolCompleto {
    addTree(miArbol);
    /*adhasdkjadhkjasdhaskda*/
    addFilePath("/home/miUsuario");
    addLegend(max, min, balanced);
}

configure miArbol {
    deleteNode(1);
}

create fotoMiArbolCompletoSin1 {
    addTree(miArbol);
    addFilePath("/home/miUsuario");
    addLegend(min, count, height);
}

